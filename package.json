{
	"name": "dayjs-banking-days",
	"version": "0.2.0",
	"author": "dayflo",
	"main": "./lib/cjs/index.js",
	"module": "./lib/esm/index.js",
	"types": "types/index.d.ts",
	"license": "MIT",
	"keywords": [
		"dayjs",
		"date",
		"holidays",
		"holiday",
		"banking holiday",
		"banking day",
		"banking days",
		"work holiday",
		"work holidays",
		"working holiday",
		"working holidays"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/dayflo/dayjs-banking-days.git"
	},
	"scripts": {
		"test": "tsx --test **/*.(test|spec).ts",
		"test:watch": "tsx --test --watch **/*.(test|spec).ts",
		"test:coverage": "tsx --test --experimental-test-coverage --test-reporter=lcov --test-reporter-destination=./coverage/lcov.info **/*.(test|spec).ts",
		"build:cjs": "tsc -p tsconfig.cjs.json",
		"build:esm": "tsc -p tsconfig.json",
		"build": "rm -rf lib && yarn build:cjs && yarn build:esm",
		"prepublishOnly": "yarn build",
		"publish": "yarn build && npm publish",
		"prettier": "prettier --write ."
	},
	"files": [
		"lib",
		"types"
	],
	"devDependencies": {
		"@types/node": "^20.11.16",
		"prettier": "^2.2.1",
		"ts-node": "^9.1.1",
		"tsx": "^4.7.0",
		"typescript": "^4.2.2"
	},
	"dependencies": {
		"dayjs": "^1.11.10"
	}
}
